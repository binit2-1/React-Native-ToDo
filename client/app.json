{
  "expo": {
    "name": "app",
    "slug": "app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "https://cdn.jsdelivr.net/gh/alohe/avatars/png/toon_1.png",
    "scheme": "app",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "https://cdn.jsdelivr.net/gh/alohe/avatars/png/toon_1.png",
        "backgroundImage": "https://cdn.jsdelivr.net/gh/alohe/avatars/png/toon_1.png",
        "monochromeImage": "https://cdn.jsdelivr.net/gh/alohe/avatars/png/toon_1.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "https://cdn.jsdelivr.net/gh/alohe/avatars/png/toon_1.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
